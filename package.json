{
  "name": "travel-app",
  "version": "0.1.0",
  "private": true,
  "packageManager": "^pnpm@6.32.4",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "vercel-build": "pnpm prisma:generate && pnpm migrate-deploy && next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:generate": "prisma generate --schema=./db/schema.prisma",
    "migrate-dev": "prisma migrate dev --preview-feature --schema=./db/schema.prisma",
    "migrate-deploy": "prisma migrate deploy --preview-feature --schema=./db/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^4.13.0",
    "@types/node": "18.16.3",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.1",
    "autoprefixer": "10.4.14",
    "eslint": "8.39.0",
    "eslint-config-next": "13.3.1",
    "next": "13.3.1",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.2",
    "ts-node": "^10.9.1",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "prisma": "^4.13.0"
  }
}